ton class=\"btn btn-primary\" @click=\"add()\">+ 추가</button>\n    </div>\n    <ul>\n      <li v-for=\"(d,idx) in state.data\" :key=\"idx\" @click=\"edit(idx)\">{{ d }}</li>\n    </ul>\n  </div>\n  <i class=\"bi bi-youtube\"></i>\n</template>\n\n<script>\nimport { reactive } from \"vue\";\nimport axios from \"axios\";\n\nexport default {\n  setup() {\n    const state = reactive({\n      data: [],\n    });\n\n    const add = () => {\n      const content = prompt(\"내용을 입력해주세요.\");\n\n      if (!content) {\n        alert(\"내용을 입력해주세요.\");\n        return add();\n      }\n\n      axios.post(\"/api/memos\", { content }).then((res) => {\n        \n        state.data = res.data;\n      });\n    };\n\n    const edit = (idx) => {\n      const content = prompt(\"내용을 입력해주세요\", state.data[idx]);\n      axios.put(\"/api/memos/\" + idx, {content  }).then((res) => {\n        state.data[idx] = res.data[idx];\n      });\n    };\n\n    axios.get(\"/api/memos\").then((res) => {\n      state.data = res.data;\n    });\n\n    return { state, add, edit };\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.memo {\n  .act {\n    padding: 10px 10px 5px 5px;\n    text-align: right;\n  }\n\n  ul {\n    list-style: none;\n    padding: 15px;\n    margin: 0;\n\n    li {\n      padding: 15px;\n      margin: 10px 0;\n      border: 1px solid #eee;\n    }\n  }\n}\n</style>\n"]}]}